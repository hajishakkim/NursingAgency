DROP TABLE IF EXISTS `jobs`;
CREATE TABLE `jobs` 
( `job_id` INT(11) NOT NULL AUTO_INCREMENT , 
`job_title` VARCHAR(500) NOT NULL , 
`job_client_id` INT(11) NOT NULL , 
`job_business_unit_id` INT(11) NOT NULL , 
`job_created_by` int(10) DEFAULT 0,
`job_created_date` date NOT NULL,
`job_updated_date` date NOT NULL,
`job_updated_by` int(11) DEFAULT 0, 
`job_status` TINYINT(1) NOT NULL DEFAULT '0' , 
 PRIMARY KEY (`job_id`))
 ENGINE = MyISAM DEFAULT CHARSET=utf8mb4;


DROP TABLE IF EXISTS `candidates`;
CREATE TABLE `candidates`(
    `candidate_id` INT(11) NOT NULL AUTO_INCREMENT,
    `candidate_fname` varchar(10) NULL,
    `candidate_lname` varchar(100) NULL,
    `candidate_title` varchar(100) NULL,
    `candidate_gender` varchar(10) NULL,
    `candidate_dob` DATE NULL,
    `candidate_address1` varchar(255) NULL,
    `candidate_address2` varchar(255) NULL,
    `candidate_address3` varchar(255) NULL,
    `candidate_country` varchar(25) NULL,
    `candidate_postcode` INT(15) NULL DEFAULT '0',
    `candidate_designation` varchar(25) NULL,
    `candidate_dbs_pvg_number` INT(11) NULL DEFAULT '0',
    `candidate_dbs_pvg_issue_date` DATE NULL ,
    `candidate_staff_id` varchar(25) NULL,
    `candidate_payroll_id` varchar(25) NULL,
    `candidate_email` varchar(50) NULL,
    `candidate_pin` INT(25) NULL DEFAULT '0',
    `candidate_pin_expiry_date` DATE NULL,
    `candidate_phone_country_code` INT(10) NULL DEFAULT '0',
    `candidate_phone_number` INT(14) NULL DEFAULT '0',
    `candidate_mobile_country_code` INT(10) NULL DEFAULT '0',
    `candidate_mobile_phone_number` INT(14) NULL DEFAULT '0',
    `candidate_employment_type` varchar(20) NULL,
    `candidate_status` TINYINT(1) NULL DEFAULT '1',
    `candidate_ni_number` INT(15) NULL DEFAULT '0',
    `candidate_passport_number` varchar(25) NULL,
    `candidate_issueing_country` CHAR(25) NULL,
    `candidate_visa_type` varchar(25) NULL,
	`candidate_activity` TINYINT(1) NULL DEFAULT '1',
    `created_date` TIMESTAMP NULL,
    PRIMARY KEY(`candidate_id`)
) ENGINE = INNODB DEFAULT CHARSET=utf8mb4;

DROP TABLE IF EXISTS `staffs`;
CREATE TABLE `staffs` (
  `staff_id` int(11) NOT NULL AUTO_INCREMENT,
  `staff_ref_id` varchar(50)  DEFAULT NULL,
  `staff_client_id` int(11) DEFAULT NULL,
  `staff_doj` VARCHAR(50) NULL DEFAULT NULL,
  `staff_shift_type` int(20) DEFAULT NULL,
  `staff_business_unit_id` int(20) DEFAULT NULL,
  `staff_space` varchar(255) DEFAULT NULL,
  `staff_location` varchar(255) DEFAULT NULL,
  `staff_details` varchar(255) DEFAULT NULL,
  `staff_activity` tinyint(1)  DEFAULT 1,
  `staff_created_by` int(10) DEFAULT 0,
  `staff_created_date` date  NULL,
  `staff_updated_date` date  NULL,
  `staff_updated_by` int(11) DEFAULT 0,
   PRIMARY KEY(`staff_id`)
) ENGINE = MyISAM DEFAULT CHARSET=utf8mb4;

DROP TABLE IF EXISTS `client_rate`;
CREATE TABLE `client_rate` (
  `client_rate_id` int(11) NOT NULL AUTO_INCREMENT,
  `client_rate_client` varchar(100) DEFAULT NULL,
  `client_rate_business_unit` varchar(100) DEFAULT NULL,
  `client_rate_job` varchar(100) DEFAULT NULL,
  `client_rate_week_days` varchar(25) DEFAULT NULL,
  `client_rate_night_time` varchar(25) DEFAULT NULL,
  `client_rate_friday_night` varchar(25) DEFAULT NULL,
  `client_rate_saturday_day` varchar(25) DEFAULT NULL,
  `client_rate_saturday_night` varchar(25) DEFAULT NULL,
  `client_rate_sunday_day` varchar(25) DEFAULT NULL,
  `client_rate_sunday_night` varchar(25) DEFAULT NULL,
  `client_rate_public_holiday_day` varchar(25) DEFAULT NULL,
  `client_rate_public_holiday_night` varchar(25) DEFAULT NULL,
  `client_rate_created_date` timestamp NULL DEFAULT NULL,
  `client_rate_updated_time` datetime DEFAULT NULL,
  `client_rate_activity` int(2) NOT NULL DEFAULT 1,
  PRIMARY KEY (`client_rate_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

  DROP TABLE IF EXISTS `staff_rate`;
  CREATE TABLE `staff_rate` (
  `staff_rate_id` int(11) NOT NULL AUTO_INCREMENT,
  `staff_rate_client` varchar(100) DEFAULT NULL,
  `staff_rate_business_unit` varchar(100) DEFAULT NULL,
  `staff_rate_job` varchar(100) DEFAULT NULL,
