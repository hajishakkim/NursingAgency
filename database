  `staff_rate_week_days` varchar(25) DEFAULT NULL,
  `staff_rate_night_time` varchar(25) DEFAULT NULL,
  `staff_rate_friday_night` varchar(25) DEFAULT NULL,
  `staff_rate_saturday_day` varchar(25) DEFAULT NULL,
  `staff_rate_saturday_night` varchar(25) DEFAULT NULL,
  `staff_rate_sunday_day` varchar(25) DEFAULT NULL,
  `staff_rate_sunday_night` varchar(25) DEFAULT NULL,
  `staff_rate_public_holiday_day` varchar(25) DEFAULT NULL,
  `staff_rate_public_holiday_night` varchar(25) DEFAULT NULL,
  `staff_rate_created_date` timestamp NULL DEFAULT NULL,
  `staff_rate_updated_time` timestamp NULL DEFAULT NULL,
  `staff_rate_activity` tinyint(1) DEFAULT 1,
  PRIMARY KEY (`staff_rate_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

  DROP TABLE IF EXISTS `document_categories`;
