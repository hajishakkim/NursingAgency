  `vaccancy_ref_number` VARCHAR(25)  NULL,
  `vaccancy_date` date  NULL,
  `vaccancy_client` int(11)  NULL,
  `vaccancy_business_unit` int(11)  NULL,
  `vaccancy_shift_type` int(11)  NULL,
  `vaccancy_job` int(11)  NULL,
  `vaccancy_break_time` date  NULL,
  `vaccancy_space` varchar(255)  NULL,
  `vaccancy_location` varchar(255)  NULL,
  `vaccancy_details` varchar(255)  NULL,
  `vaccancy_candidate_id`  int(11)  NULL,
  `vaccancy_acceptance`int(11)  NULL,
  `vaccancy_created_time` date  NULL,
  `vaccancy_updated_time` date  NULL,
  `vaccancy_created_by` int(11)  NULL,
  `vaccancy_updated_by` date  NULL,
  `vaccancy_active` TINYINT(1) NULL DEFAULT '1'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

